{
  "providers": ["python"],
  "phases": {
    "setup": {
      "nixPkgs": ["python311", "python311Packages.pip"]
    },
    "install": {
      "cmds": ["pip install -r requirements.txt"]
    },
    "build": {
      "cmds": ["python docker_init.py"]
    }
  },
  "start": {
    "cmd": "uvicorn app.main:app --host 0.0.0.0 --port $PORT"
  }
}
